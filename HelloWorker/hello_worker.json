{
  "worker_type": "HelloWorker",
  "layer": "greetings",
  "entity_interest": {
    "range_entity_interest": {
      "radius": 2
    }
  },
  "component_delivery": {
    "default": "RELIABLE_ORDERED",
    "checkout_all_initially": true
  },
  "launch_configuration": {
    "cloud_deployment": {
      "linux": {
        "local_build_directory": "bin/x64/ReleaseLinux",
        "command": "mono",
        "arguments": [
          "HelloWorker.exe",
          "${IMPROBABLE_RECEPTIONIST_HOST}",
          "${IMPROBABLE_RECEPTIONIST_PORT}",
          "${IMPROBABLE_WORKER_ID}"
        ]
      }
    },
    "local_deployment": {
      "macos": {
        "local_build_directory": "bin/x64/ReleaseMacOS",
        "command": "mono",
        "arguments": [
          "--arch=64",
          "HelloWorker.exe",
          "${IMPROBABLE_RECEPTIONIST_HOST}",
          "${IMPROBABLE_RECEPTIONIST_PORT}",
          "${IMPROBABLE_WORKER_ID}"
        ]
      },
      "linux": {
        "local_build_directory": "bin/x64/ReleaseLinux",
        "command": "mono",
        "arguments": [
          "HelloWorker.exe",
          "${IMPROBABLE_RECEPTIONIST_HOST}",
          "${IMPROBABLE_RECEPTIONIST_PORT}",
          "${IMPROBABLE_WORKER_ID}"
        ]
      },
      "windows": {
        "local_build_directory": "bin/x64/ReleaseWindows",
        "command": "HelloWorker.exe",
        "arguments": [
          "${IMPROBABLE_RECEPTIONIST_HOST}",
          "${IMPROBABLE_RECEPTIONIST_PORT}",
          "${IMPROBABLE_WORKER_ID}"
        ]
      }
    }
  }
}
